<h1>All Products Index Page</h1>
<div class="container">
    <div class="row">
        <div class="col-xs-6">
            <ul>
                <% @products.each do |product| %>
                    <li><%= link_to(product.name, product) %></li>
                <% end %>
            </ul>
        </div>
    </div>
</div>
